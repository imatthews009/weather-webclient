<h1><%=@city%> Weather</h1>

<%= form_tag "/", method: :get do %>
    <div>
        City: <%= text_field_tag :city %>
        State: <%= text_field_tag :state %>
        <%= submit_tag "Submit" %>
    </div>
<% end %>


<h2><%= @sftemp %> Degress</h2>
<% @forecast_days[0..4].each do |forecast| %>
    <h2><%= forecast["date"]%></h2>
    <h2>High<%= forecast["high"]%></h2>
    <h2>Low<%= forecast["low"]%></h2>
    <h2>Low<%= forecast["text"]%></h2>
    
<% end %>
